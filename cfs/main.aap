:child cfslib/main.aap
:child interfaces/fuse/main.aap
INCLUDE = -I ./include/ -I ./cfslib/ -I ./bstring/ -I ./interfaces/fuse/
DEBUG=yes

INTERFACES=fuse

all: libcfs

interfaces-all {virtual}: $*INTERFACES

